{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/app/screen/auth/JwtCoantext.js",
    "src/app/screen/auth/useAuthContext.js",
    "src/app/screen/auth/AuthLoginForm.js",
    "src/app/screen/detailproduct/layout.js",
    "src/app/screen/dashboard/layout.js",
    "src/app/screen/register/AuthRegisterForm.js"
  ],
  "exclude": ["node_modules"]
}
